    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "rxjs": "~6.3.3"
  },
  "devDependencies": {
    "ng-packagr": "3.0.0-rc.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1"
